<%- include('_header',{title : "All Product" }) -%>
<body>
    <div>
        <h1>All Products</h1>
        <% products.forEach(product =>{ %>
            <p onclick="clickedProductName(`<%= product.name %>`)">
                <%= product.name %>
            </p>
            <p><%= product.price %></p>

            <p><a href="products/<%= product._id %>">Click Here</a></p>
        <% }) %>
    </div>
   
<script src="js/product.js">
  
</script>

</body>
</html>
